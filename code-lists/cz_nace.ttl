        skos:inScheme    <http://linked.opendata.cz/resource/concept-scheme/nace> ;
        skos:narrower    <http://linked.opendata.cz/resource/concept-scheme/nace/61104> , <http://linked.opendata.cz/resource/concept-scheme/nace/61103> , <http://linked.opendata.cz/resource/concept-scheme/nace/61109> , <http://linked.opendata.cz/resource/concept-scheme/nace/61101> , <http://linked.opendata.cz/resource/concept-scheme/nace/61102> ;
